server:
  port: 8080

spring:
  application:
    name: productiviyApp-api-gateway

security:
  oauth2:
    resource:
      jwt:
        key-value: laser123
zuul:
  routes:
    user-service:
      path: /user/**
      url: http://localhost:8081/api/v1/user-service/

eureka:
  client:
    serviceUrl:
      defaultZone: http://localhost:8761/eureka/
    healthcheck:
      enabled: true
