app:
  uri: /api/v1/user-service

server:
  port: 8081

jwt:
  secret: JWT_SECRET
  expirationMs: 86400000

spring:
  datasource:
    url: jdbc:mysql://DB_HOST:DB_PORT/DB_NAME
    username: DB_USER
    password: DB_PASS
    driver-class-name: com.mysql.cj.jdbcproject created.Driver
  jpa:
    hibernate:
      ddl-auto: update
    properties:
      hibernate:
        show_sql: true
        format_sql: true
        globally_quoted_identifiers: true